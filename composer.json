{
    "name": "wedesignit/parcel-pro-php-api-client",
    "description": "PHP Client for the Parcel Pro API",
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "^6|^7",
        "php": "^7.1|^8.0",
        "ext-json": "*"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick van Kouteren",
            "email": "p.vankouteren@wedesignit.nl",
            "homepage": "https://www.wedesignit.nl"
        }
    ],
    "autoload": {
        "psr-4": {
            "WeDesignIt\\ParcelPro\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
